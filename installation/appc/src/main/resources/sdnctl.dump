BEGIN
    declare reftransid varchar(128);
    declare ccount int;
    declare ahostip varchar(45);
    declare ahdcode varchar(45);
    declare ahdendtime datetime;
    declare ahdstarttime datetime;
    declare acacheexpiretime datetime;
    declare hdexectime INT DEFAULT 5;
    declare hdexpectedendtime datetime;
    declare nowtime datetime;
    declare acacheduration INT;
    declare maxtimeintbl INT DEFAULT 1800;
    
    SET nowtime:=utc_timestamp();
    SET ahdcode:=NEW.HCCODE;
    SET ahostip:=NEW.HOSTIP;
    SET reftransid:=trim(NEW.REFHCTRANSID);
    SET new.CREATEDON = nowtime;
    SET new.HDSTARTTIME = nowtime;
    
    delete from OPS_HCRESPCACHE where HDENDTIME is null AND time_to_sec(timediff(nowtime, HDSTARTTIME))>maxtimeintbl;
    
    IF (reftransid is null) then
        select CACHEDURATION, EXECTIME into acacheduration, hdexectime from OPS_HCTYPES where HCCODE=ahdcode;
        select count(*) into ccount from OPS_HCRESPCACHE where HCCODE=ahdcode and hostip=ahostip AND HDENDTIME is not null AND HCRESPONSEID>0;
        if (ccount > 0) then
            select HDSTARTTIME, HDENDTIME, CACHEEXPIRETIME into ahdstarttime, ahdendtime, acacheexpiretime from OPS_HCRESPCACHE where
                HCCODE=ahdcode and hostip=ahostip AND HDENDTIME is not null AND HCRESPONSEID>0 order by HDENDTIME desc limit 1 ;
            select time_to_sec(timediff(ahdendtime, ahdstarttime)) into hdexectime;
        end if;

        if (acacheduration > 0) then
            set hdexpectedendtime = nowtime + INTERVAL hdexectime second;
            insert into OPS_HCRESPCACHE(HCRESPONSEID,SDNOTRANSID,HOSTIP,HCRESPONSESTATUS,HCRESPONSE,CREATEDON,SDNOINSTANCE,SDNOINSTANCESERVER,DMAAPSENT,
                   NODETYPE,HCCODE,USERID,CLIENTNAME,HDSTARTTIME,HDENDTIME,CACHEEXPIRETIME,HDEXPECTEDENDTIME)
                VALUES(NEW.HCRESPONSEID,NEW.SDNOTRANSID,NEW.HOSTIP,null,null,nowtime,NEW.SDNOINSTANCE,NEW.SDNOINSTANCESERVER,NEW.DMAAPSENT,
                   NEW.NODETYPE,NEW.HCCODE,NEW.USERID,NEW.CLIENTNAME,NEW.HDSTARTTIME,null,null,hdexpectedendtime);
