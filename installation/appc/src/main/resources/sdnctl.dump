            select ahdendtime + INTERVAL aduration MINUTE into cachetime from OPS_HCTYPES where HCCODE=ahdcode;     
            update OPS_HCRESPCACHE 
                set HCRESPONSESTATUS=NEW.HCRESPONSESTATUS,HCRESPONSE=NEW.HCRESPONSE,HDENDTIME=NEW.HDENDTIME,CACHEEXPIRETIME=cachetime,HDEXPECTEDENDTIME=null
                where SDNOTRANSID=NEW.SDNOTRANSID and CREATEDON=NEW.CREATEDON;
