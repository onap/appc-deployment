        update OPS_HCRESPCACHE 
                set DMAAPSENT=NEW.DMAAPSENT where OLD.SDNOTRANSID=NEW.SDNOTRANSID and OLD.CREATEDON=NEW.CREATEDON;
    ELSEIF (reftransid is null and OLD.HCRESPONSESTATUS is null) then
        select cacheduration into aduration from OPS_HCTYPES where HCCODE=ahdcode; 
